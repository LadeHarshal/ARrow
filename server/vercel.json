{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["./routes/**/*"]
        }
      }
    ],
    "routes": [
        { "src": "/[^.]+", 
          "status": 302,
          "continue": true
          }
        ]
}
