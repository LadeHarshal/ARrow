{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["./routes/**/*"]
        }
      }
    ],
    "routes": [{ "src": "/[^.]+", "status": 302, "headers": {
        "Location": "https://accounts.google.com/auth",
      }  "continue": true}]
}
